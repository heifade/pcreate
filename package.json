{
  "name": "pcreate",
  "version": "1.0.0-beta108",
  "description": "",
  "main": "./es/index.js",
  "module": "./es/index.js",
  "typings": "./es/index",
  "files": [
    "es"
  ],
  "aaabin": {
    "pcreate": "./es/bin/pcreate",
    "pc": "./es/bin/pcreate"
  },
  "scripts": {
    "clean": "node ./tools/clean.js",
    "code-build": "node ./tools/copy.js && tsc -p tsconfig.json",
    "docs": "typedoc --out docs src --module commonjs --hideGenerator && node ./tools/formatDocs.js",
    "build": "npm run clean && npm run code-build && npm run docs",
    "test": "nyc mocha -t 5000",
    "test-nyc": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "start-new": "npm run build && clear && node ./es/index.js new project -p ..",
    "start-build": "npm run build && clear && node ./es/index.js build -p ../p1"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/heifade/pcreate.git"
  },
  "keywords": [],
  "author": "heifade",
  "license": "MIT",
  "dependencies": {
    "browserify": "^15.0.0",
    "chalk": "^2.3.0",
    "coveralls": "^3.0.0",
    "fs-i": "^1.1.3",
    "inquirer": "^5.0.0",
    "lodash": "^4.17.4",
    "mocha": "^4.0.1",
    "nyc": "^11.3.0",
    "pcreate-config": "^1.0.7",
    "ts-node": "^4.1.0",
    "tsify": "^3.0.4",
    "typedoc": "^0.9.0",
    "typedoc-format": "1.0.4",
    "typescript": "^2.6.2",
    "update": "^0.7.4",
    "yargs": "^10.1.0",
    "zip-i": "1.0.8"
  },
  "devDependencies": {
    "@types/browserify": "^12.0.33",
    "@types/chai": "^4.0.5",
    "@types/inquirer": "0.0.36",
    "@types/mocha": "^2.2.44",
    "@types/yargs": "^10.0.0",
    "chai": "^4.1.2",
    "source-map-support": "^0.5.0"
  }
}